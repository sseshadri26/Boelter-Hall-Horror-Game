                <ui:Button text="Main Menu" display-tooltip-when-elided="true" name="main-menu-button" class="boelter-simple-button" style="font-size: 65px;" />
                <ui:Button text="Settings" display-tooltip-when-elided="true" name="settings-button" class="boelter-simple-button" style="font-size: 65px;" />
            </ui:VisualElement>
