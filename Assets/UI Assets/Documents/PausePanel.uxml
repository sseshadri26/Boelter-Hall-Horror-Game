<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:Template name="TitleBlock" src="project://database/Assets/UI%20Assets/Documents/ComponentsUXML/TitleBlock.uxml?fileID=9197481963319205126&amp;guid=9ea99844e54943a4f808804ab30e1bd8&amp;type=3#TitleBlock" />
    <Style src="project://database/Assets/UI%20Assets/Styles/boelter-game.uss?fileID=7433441132597879392&amp;guid=1206311c1593adc44a6dea0891905f54&amp;type=3#boelter-game" />
    <Style src="project://database/Assets/Resources/UI%20Styles/Panel-animations.uss?fileID=7433441132597879392&amp;guid=908ee5a3615766e4e86cbc850aa00098&amp;type=3#Panel-animations" />
    <ui:VisualElement name="Panel" style="width: 1920px; height: 1080px; background-color: rgb(38, 39, 38); border-top-left-radius: 10px; border-bottom-left-radius: 10px; border-top-right-radius: 10px; border-bottom-right-radius: 10px;">
        <ui:VisualElement name="Border-top" style="height: 40px;" />
        <ui:VisualElement name="Row1" style="height: auto; flex-direction: row;">
            <ui:VisualElement name="Spacer-Left" style="width: 35%;" />
            <ui:Instance template="TitleBlock" name="PauseMenuTitleBlock" style="flex-grow: 1;">
                <AttributeOverrides element-name="TitleText" text="Paused" />
            </ui:Instance>
            <ui:VisualElement name="Spacer-Right" style="width: 35%;" />
        </ui:VisualElement>
        <ui:VisualElement name="Row2" style="flex-grow: 1; align-items: center;">
            <ui:VisualElement name="ButtonsBox" style="width: 1649px; height: auto; margin-left: 20px; margin-right: 20px; margin-top: 20px; margin-bottom: 20px; border-left-color: rgb(152, 152, 152); border-right-color: rgb(152, 152, 152); border-top-color: rgb(152, 152, 152); border-bottom-color: rgb(152, 152, 152); border-left-width: 2px; border-right-width: 2px; border-top-width: 2px; border-bottom-width: 2px;">
